<p id="notice"><%= notice %></p>

<h1>Orden Mantenimientos</h1>

<table class="table table-responsive">
  <thead>
    <tr>
      <th>Vehiculo placa</th>
      <th>Tipo</th>
      <th>Kilometraje</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orden_mantenimientos.each do |orden_mantenimiento| %>
      <tr>
        <td><%= orden_mantenimiento.vehiculo_placa %></td>
        <td><%= orden_mantenimiento.tipo %></td>
        <td><%= orden_mantenimiento.kilometraje %></td>
        <td><%= link_to 'Show', orden_mantenimiento %></td>
        <td><%= link_to 'Edit', edit_orden_mantenimiento_path(orden_mantenimiento) %></td>
        <td><%= link_to 'Destroy', orden_mantenimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Orden Mantenimiento', new_orden_mantenimiento_path %>
